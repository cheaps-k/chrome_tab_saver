{
    "manifest_version": 2,
    "name": "Tab Shelf",
    "description": "Save and restore the opening tabs.",
    "version": "1.0.2",
    "default_locale": "en",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}
